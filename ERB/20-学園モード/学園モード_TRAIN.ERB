	CASEELSE
		;#;THROW 「%場所%」のコマンドフィルターが設定されていない
ENDSELECT

FOR LCOUNT, 0, VARSIZE("TRAINNAME")
	SELECTCASE TRAINNAME:LCOUNT
		;ロケーション関係なく実行可能
		{
